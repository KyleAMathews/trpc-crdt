{
  "name": "trpc-yjs",
  "description": "Call server functions natively in your Yjs app with tRPC",
  "version": "0.0.9",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "devDependencies": {
    "@trpc/client": "^10.38.5",
    "@trpc/server": "^10.38.5",
    "yjs": "^13.6.8"
  },
  "exports": {
    "./adapter": "./dist/adapter.js",
    "./link": "./dist/link.js"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "keywords": [],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "yjs": "*",
    "@trpc/client": "*",
    "@trpc/server": "*"
  },
  "scripts": {
    "build": "npm run clean && tsup && npm run post-build",
    "check": "tsc",
    "clean": "shx rm -rf dist *.d.ts",
    "post-build": "shx mv dist/*.d.ts* .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "tsup": {
    "entry": [
      "src/adapter.ts",
      "src/link.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true
  }
}
