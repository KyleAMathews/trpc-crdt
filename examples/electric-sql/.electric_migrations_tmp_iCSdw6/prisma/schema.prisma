generator client {
  provider = "prisma-client-js"
}

generator electric {
  provider      = "/Users/kylemathews/programs/trpc-crdt/examples/electric-sql/node_modules/@electric-sql/prisma-generator/dist/bin.js"
  output        = "/Users/kylemathews/programs/trpc-crdt/examples/electric-sql/src/generated/client"
  relationModel = "false"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DB_URL")
}

model TrpcCalls {
  @@map("trpc_calls")
  id        String  @id
  createdat String
  elapsedms Int?
  path      String
  input     String?
  type      String
  error     Int
  done      Int
  clientid  String
  response  String?
}

model Users {
  @@map("users")
  id   String @id
  name String
}
